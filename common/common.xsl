<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">	<xsl:import href="xsl/attributs.xsl"/>	<xsl:import href="xsl/builder.xsl"/>	<xsl:import href="xsl/builder_display.xsl"/>	<xsl:import href="xsl/contact.xsl"/>	<xsl:import href="xsl/dates.xsl"/>	<xsl:import href="xsl/form.xsl"/>	<xsl:import href="xsl/html_head.xsl"/>	<xsl:import href="xsl/image.xsl"/>	<xsl:import href="xsl/label.xsl"/>	<xsl:import href="xsl/liens.xsl"/>	<xsl:import href="xsl/lightbox.xsl"/>	<xsl:import href="xsl/listing.xsl"/>	<xsl:import href="xsl/navigation.xsl"/>	<xsl:import href="xsl/search.xsl"/>	<xsl:import href="xsl/specific_navigation.xsl"/>	<xsl:import href="xsl/specific_string.xsl"/>	<xsl:import href="xsl/text.xsl"/>	<xsl:import href="xsl/titrage.xsl"/>	<xsl:import href="xsl/value.xsl"/>		<xsl:output method="xml" indent="yes" encoding="utf-8" doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN" doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd" omit-xml-declaration="yes"/>	<!-- -->	<!-- paramètres utiles partout -->	<xsl:param name="majuscules">ABCDEFGHIJKLMNOPQRSTUVWXYZÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞ</xsl:param>	<xsl:param name="minuscules">abcdefghijklmnopqrstuvwxyzàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþ</xsl:param>	<xsl:param name="sans_accent">abcdefghijklmnopqrstuvwxyzaaaaaaæceeeeiiiidnoooooouuuuyp</xsl:param>	<xsl:param name="apos">		<xsl:text>'</xsl:text>	</xsl:param>	<xsl:param name="new_apos">		<xsl:text>’</xsl:text>	</xsl:param>	<!--xsl:param name="public">http://pfb.officity.com/admin/</xsl:param-->	<xsl:param name="files" select="/RESPONSE/NECTIL/files_url"/>	<xsl:param name="common" select="concat(/RESPONSE/NECTIL/host, '/common/')"/>	<xsl:param name="public" select="/RESPONSE/NECTIL/public_url"/>	<xsl:param name="months" select="/RESPONSE/MONTHS"/>	<!-- AJOUTER JOURS - COUNTRIES - ENUM/E ? -->	<xsl:param name="website" select="/RESPONSE/RESULTS[@name='website']/MEDIA"/>	<xsl:param name="media" select="/RESPONSE/RESULTS[@name='media']/MEDIA"/>	<xsl:param name="data" select="/RESPONSE/RESULTS[@name='data']/MEDIA"/>	<xsl:param name="ID" select="/RESPONSE/URL/ID"/>	<!-- -->	<!-- -->	<!-- -->	<!-- -->	<!-- -->	<!-- -->	<!-- -->	<!-- FONCTIONS GENERIQUES -->	<!-- -->	<!-- haut de la page -->	<!-- introduire un template name pour généraliser la requête ? ne s'applique a priori que sur le website -->	<xsl:template match="MEDIA" mode="top">		<div id="top">			<div id="navigation">				<xsl:apply-templates select="." mode="title"/>				<ul>					<xsl:apply-templates select="DEPENDENCIES/DEPENDENCY[@type='mediaNavigation']/MEDIA" mode="navigation_item"/>				</ul>			</div>			<br class="break"/>		</div>	</xsl:template>	<!-- contenu - media affiché sur la page -->	<xsl:template match="MEDIA" mode="content">		<div id="content">			<xsl:apply-templates select="." mode="h1"/>			<xsl:apply-templates select="." mode="body"/>			<xsl:apply-templates select="." mode="specific"/>			<hr class="clear"/>		</div>	</xsl:template>	<!-- dans le contenu - preview du media -->	<xsl:template match="MEDIA" mode="in_content">		<div>			<xsl:apply-templates select="." mode="attribute"/>			<xsl:apply-templates select="." mode="h2"/>			<xsl:apply-templates select="." mode="preview"/>		</div>	</xsl:template>	<!-- -->	<!-- contenu lateral -->	<xsl:template match="MEDIA" mode="side_content">		<xsl:param name="dependency_type"/>		<div id="side_{$depednecy_type}">			<xsl:apply-templates select="DEPENDENCIES/DEPENDENCY[ @type= $dependency_type ]/MEDIA[@ID != //URL/ID]" mode="in_content"/>		</div>	</xsl:template>	<!-- -->	<!-- bas de la page -->	<xsl:template match="MEDIA" mode="top">		<div id="top">			<div id="navigation">				<xsl:apply-templates select="." mode="title"/>				<ul>					<xsl:apply-templates select="DEPENDENCIES/DEPENDENCY[@type='mediaNavigation']/MEDIA" mode="navigation_item"/>				</ul>			</div>			<br class="break"/>		</div>	</xsl:template>	</xsl:stylesheet>