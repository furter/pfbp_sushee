<?xml version="1.0"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">	<xsl:import href="common.xsl"/>	<xsl:template match="/RESPONSE">		<html>			<head>				<title>Prix Fernand Baudin - Liste des soumissions</title>				<style>					body{ font-family:Helvetica, Arial, Sans-Serif;}					table, tr, td, th {border:1px solid black; border-collapse:collapse; vertical-align:top; padding:2px;font-size:11px; text-align:left;}					a img{border:none; margin-right:3px;}				</style>			</head>			<body>				<!--xsl:apply-templates select="$media" mode="h1"/-->				<xsl:apply-templates select="/RESPONSE/CONFIG/FORM[@name='BrowseContacts']" mode="config"/>				<xsl:apply-templates select="/RESPONSE/DISPLAY[@module='CONTACT']" mode="display_options"/>				<xsl:apply-templates select="/RESPONSE/RESULTS[@name='data']" mode="display"/>			</body>		</html>	</xsl:template>	<xsl:template match="RESULTS" mode="display">		<xsl:apply-templates select="." mode="pagination"/>		<table id="listing">			<tr>				<xsl:apply-templates select="//DISPLAY[@module='CONTACT']/*" mode="display_header"/>			</tr>			<xsl:apply-templates select="*" mode="data"/>		</table>	</xsl:template></xsl:stylesheet>