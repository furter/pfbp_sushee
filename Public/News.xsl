<?xml version="1.0"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">	<xsl:import href="common.xsl"/>	<xsl:template match="/RESPONSE">		<xsl:call-template name="html"/>	</xsl:template>	<xsl:template match="MEDIA" mode="specific">		<div id="specific">			<xsl:apply-templates select="$data" mode="in_content"/>		</div>	</xsl:template>	<xsl:template match="MEDIA" mode="in_content">		<div id="m{@ID}">			<xsl:apply-templates select="." mode="attribute"/>			<div class="visualNews">				<xsl:apply-templates select="." mode="visual">					<xsl:with-param name="width" select="270"/>				</xsl:apply-templates>			</div>			<div class="body_container">				<xsl:apply-templates select="." mode="h2"/>				<xsl:apply-templates select="." mode="header"/>				<!--xsl:apply-templates select="." mode="body"/-->			</div>			<hr class="clear"/>		</div>	</xsl:template>	<xsl:template match="MEDIA[INFO/MEDIATYPE = 'News']" mode="content">		<div id="content">			<h1>News</h1>			<xsl:apply-templates select="." mode="specific"/>		</div>	</xsl:template>	</xsl:stylesheet>