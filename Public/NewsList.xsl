<?xml version="1.0"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">	<xsl:import href="common.xsl"/>	<xsl:template match="/RESPONSE">		<xsl:call-template name="html"/>	</xsl:template>	<xsl:template match="MEDIA" mode="specific">		<div id="specific">			<xsl:apply-templates select="$data" mode="in_content"/>		</div>	</xsl:template>	<xsl:template match="MEDIA" mode="in_content">		<div id="m{@ID}">			<xsl:apply-templates select="." mode="attribute"/>			<div class="visualNews">				<xsl:apply-templates select="." mode="visual">					<xsl:with-param name="width" select="270"/>				</xsl:apply-templates>				<xsl:if test="DESCRIPTIONS/DESCRIPTION/CUSTOM/VISUAL_FOLDER != ''">					<xsl:for-each select="DESCRIPTIONS/DESCRIPTION/CUSTOM/TREE/FILE">						<xsl:sort select="@name" data-type="text" order="ascending"/>						<img src="/common/img_resize.php?path={@path}&amp;width=270"/>					</xsl:for-each>					<xsl:if test="DESCRIPTIONS/DESCRIPTION/COPYRIGHT != ''">						<p>							<xsl:value-of select="DESCRIPTIONS/DESCRIPTION/COPYRIGHT"/>						</p>					</xsl:if>				</xsl:if>				<xsl:if test="DEPENDENCIES/DEPENDENCY/MEDIA[INFO/MEDIATYPE = 'Image']">					<xsl:for-each select="DEPENDENCIES/DEPENDENCY/MEDIA[INFO/MEDIATYPE = 'Image']">						<div>							<xsl:apply-templates select="." mode="visual">								<xsl:with-param name="visual" select="DESCRIPTIONS/DESCRIPTION/CUSTOM/Visual"/>								<xsl:with-param name="width" select="270"/>							</xsl:apply-templates>							<xsl:apply-templates select="." mode="header"/>							<xsl:if test="DESCRIPTIONS/DESCRIPTION/COPYRIGHT != ''">								<p>									<xsl:value-of select="DESCRIPTIONS/DESCRIPTION/COPYRIGHT"/>								</p>							</xsl:if>						</div>					</xsl:for-each>				</xsl:if>			</div>			<div class="body_container">				<h6>					<xsl:call-template name="date">						<xsl:with-param name="date" select="INFO/CREATIONDATE"/>					</xsl:call-template>				</h6>				<xsl:apply-templates select="." mode="h2"/>				<xsl:apply-templates select="." mode="header"/>				<xsl:apply-templates select="." mode="body"/>			</div>			<hr class="clear"/>		</div>	</xsl:template></xsl:stylesheet>